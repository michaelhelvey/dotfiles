{
  "editor.fontSize": 12,
  "editor.lineHeight": 1.4,
  "editor.minimap.enabled": false,
  "editor.fontFamily": "'MonoLisa', 'Cascadia Code', 'JetBrainsMono Nerd Font', 'FiraCode NF', 'Consolas', monospace",
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "none",
  "window.menuBarVisibility": "toggle",
  "editor.fontWeight": "normal",
  "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.lineHeight": 1,
  "terminal.integrated.fontFamily": "Cascadia Code",
  "terminal.integrated.env.linux": {
    "IS_VSCODE": "true"
  },
  "typescript.suggest.autoImports": true,
  "workbench.settings.editor": "json",
  "[markdown]": {
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 100,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.quickSuggestions": false
  },
  "editor.formatOnSave": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditor"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["f", "d"],
      "after": ["<Esc>"]
    }
  ],
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.fontSize": 16,
  "prettier.proseWrap": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "advancedNewFile.exclude": {
    "coverage": true
  },
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
  "jest.showCoverageOnLoad": false,
  "jest.jestCommandLine": "npx -w packages/api jest",
  "jest.coverageFormatter": "DefaultFormatter",
  "jest.coverageColors": {
    "covered": "rgba(9, 156, 65, 0.4)",
    "uncovered": "rgba(121, 31, 10, 0.4)",
    "partially-covered": "rgba(235, 198, 52, 0.4)"
  },
  "jest.autoRun": "off",
  "gitlens.currentLine.enabled": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "workbench.colorTheme": "GitHub Light"
}
